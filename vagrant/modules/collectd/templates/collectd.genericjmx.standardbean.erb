		<MBean "<%= title %>-memory">
			ObjectName "java.lang:type=Memory"
			InstancePrefix "<%= title %>.memory"
			<Value>
				Type		memory
				Table		true
				Attribute	HeapMemoryUsage
				InstancePrefix  "heap"
			</Value>
			<Value>
				Type		memory
				Table		true
				Attribute	NonHeapMemoryUsage
				InstancePrefix  "nonheap"
			</Value>
			<Value>
				Type		gauge
				Attribute	ObjectPendingFinalizationCount
				InstancePrefix	"pending"
			</Value>
		</MBean>
		<MBean "<%= title %>-mempool">
			ObjectName	"java.lang:type=MemoryPool,name=*"
			InstancePrefix	"<%= title %>.mempool."
			InstanceFrom	name
			<Value>
				Type		memory
				Table		true
				Attribute	Usage
				InstancePrefix	"usage"
			</Value>
			<Value>
				Type		memory
				Table		true
				Attribute	PeakUsage
				InstancePrefix	"peakusage"
			</Value>
		</MBean>
		<MBean "<%= title %>-gc">
			ObjectName	"java.lang:type=GarbageCollector,name=*"
			InstancePrefix	"<%= title %>.gctime."
			InstanceFrom	name
			<Value>
				Type		memory
				Attribute	CollectionTime
				InstancePrefix	"collection-time"
			</Value>
		</MBean>
		<MBean "<%= title %>-os">
			ObjectName	"java.lang:type=OperatingSystem"
			InstancePrefix	"<%= title %>.os"
			<Value>
				Type		gauge
				Attribute	OpenFileDescriptorCount
				InstancePrefix	"filedes.open"
			</Value>
			<Value>
				Type		gauge
				Attribute	MaxFileDescriptorCount
				InstancePrefix	"filedes.max"
			</Value>
			<Value>
				Type		memory
				Attribute	CommittedVirtualMemorySize
				InstancePrefix	"memcommit"
			</Value>
			<Value>
				Type		memory
				Attribute	FreePhysicalMemorySize
				InstancePrefix	"memphy.free"
			</Value>
			<Value>
				Type		memory
				Attribute	TotalPhysicalMemorySize
				InstancePrefix	"memphy.total"
			</Value>
			<Value>
				Type		memory
				Attribute	FreeSwapSpaceSize
				InstancePrefix	"swap.tree"
			</Value>
			<Value>
				Type		memory
				Attribute	TotalSwapSpaceSize
				InstancePrefix	"swap.total"
			</Value>
			<Value>
				Type		gauge
				Attribute	SystemLoadAverage
				InstancePrefix	"loadavg"
			</Value>
		</MBean>
		<MBean "<%= title %>-classloading">
			ObjectName	"java.lang:type=ClassLoading"
			InstancePrefix	"<%= title %>.classes"
			<Value>
				Type		gauge
				Attribute	LoadedClassCount
				InstancePrefix	"loaded"
			</Value>
			<Value>
				Type		gauge
				Attribute	UnloadedClassCount
				InstancePrefix	"unloaded"
			</Value>
			<Value>
				Type		gauge
				Attribute	TotalLoadedClassCount
				InstancePrefix	"total"
			</Value>
		</MBean>
		<MBean "<%= title %>-compilation">
			ObjectName	"java.lang:type=Compilation"
			InstancePrefix	"<%= title %>.compilation"
			<Value>
				Type		gauge
				Attribute	TotalCompilationTime
				InstancePrefix	"time"
			</Value>
		</MBean>
		<MBean "<%= title %>-runtime">
			ObjectName	"java.lang:type=Runtime"
			InstancePrefix	"<%= title %>.runtime"
			<Value>
				Type		gauge
				Attribute	StartTime
				InstancePrefix	"starttime"
			</Value>
			<Value>
				Type		gauge
				Attribute	Uptime
				InstancePrefix	"uptime"
			</Value>
		</MBean>
		<MBean "<%= title %>-cpu">
			ObjectName	"java.lang:type=Threading"
			InstancePrefix	"<%= title %>.cpu"
			<Value>
				Type		gauge
				Attribute	ThreadCount
				InstancePrefix	"threads.count"
			</Value>
			<Value>
				Type		gauge
				Attribute	DaemonThreadCount
				InstancePrefix	"threads.daemon"
			</Value>
			<Value>
				Type		gauge
				Attribute	TotalStartedThreadCount
				InstancePrefix	"threads.total"
			</Value>
			<Value>
				Type		derive
				Attribute	CurrentThreadCpuTime
				InstancePrefix	"cpu"
			</Value>
			<Value>
				Type		derive
				Attribute	CurrentThreadUserTime
				InstancePrefix	"user"
			</Value>
		</MBean>
